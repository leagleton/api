{
    "/products": {
        "get": {
            "tags": [
                "Products"
            ],
            "summary": "Fetch a product or products from WinMan",
            "operationId": "getProducts",
            "description": "<p>Fetch web-enabled products from WinMan.</p>",
            "produces": [
                "application/json",
                "application/xml"
            ],
            "parameters": [
                {
                    "in": "query",
                    "name": "sku",
                    "description": "<p>Fetch a single web-enabled product with the supplied <em>sku</em>.</p>",
                    "required": false,
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 100
                },
                {
                    "in": "query",
                    "name": "website",
                    "description": "<p>Fetch only web-enabled products which are attached to the supplied <em>website</em>.</p>",
                    "required": true,
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 100
                },
                {
                    "in": "query",
                    "name": "modified",
                    "description": "<p>Fetch web-enabled products modified within the specified number of seconds.</p>",
                    "required": false,
                    "type": "integer",
                    "format": "int32",
                    "minimum": 0
                }
            ],
            "responses": {
                "200": {
                    "description": "Query executed successfully.",
                    "schema": {
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/Product"
                        },
                        "xml": {
                            "name": "Products",
                            "wrapped": true
                        }
                    }
                },
                "400": {
                    "description": "Bad input parameter.",
                    "schema": {
                        "$ref": "#/definitions/ErrorResponse"
                    }
                },
                "401": {
                    "description": "Unauthorised. Access token may be invalid, expired or not supplied in the request."
                }                
            }
        }
    }
}