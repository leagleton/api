            "CustomerPricingClassification": {
                "$ref": "#/definitions/CustomerPricingClassification"
            },
            "CrossReferences": {
                "type": "array",
                "items": {
                    "$ref": "#/definitions/CrossReference"
                },
                "xml": {
                    "name": "CrossReferences",
                    "wrapped": true
                }
            },
            "Notes": {
                "type": "string",
                "example": "",
                "description": "Maps to the fields <em>Customers.Notes</em> in your WinMan database."
            },
            "PromptText": {
                "type": "string",
                "example": "",
                "description": "Maps to the fields <em>Customers.PromptText</em> in your WinMan database."
            }
        }
    }
}