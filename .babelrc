{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    [
      "module-alias",
      [
        {
          "expose": "components",
          "src": "frontend/swagger/core/components"
        },
        {
          "expose": "core",
          "src": "frontend/swagger/core"
        },
        {
          "expose": "corePlugins",
          "src": "frontend/swagger/core/plugins"
        }
      ]
    ]
  ]
}
